{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "if": {
      "properties": { "BlockType": { "const": "text" } }
    },
    "then" : {
        "properties": {
            "TextOptions": { "$ref" : "TextOptions.json" },
            "TextOptionsExtern" : {
                "type" : "string",
                "enum" : [""],
                "errorMessage" : "TextOptionsExtern is deprecated. Use Styles"
            }
        }
    }
}