{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "Category" : {
        "type" : "string",
        "enum" : [
          "Nation",
          "Game",
          "Vendor",
          "CarClass",
          "Car",
          "Team",
          "Track",
          "Championship",
          "Season",
          "Event",
          "Driver"
        ]
      },
      "Variant" : {
        "type" : "string",
        "enum" : [
            "default",
            "alternative",
            "dark",
            "light",
            "grayed"
        ]
      },
      "UseFallbackVariant" : {
        "type" : "boolean",
        "default" : true
      },
      "UseFallbackSource" : {
        "type" : "boolean",
        "default" : true
      },
      "SetVariantByDefault" : {
        "type" : "boolean",
        "default" : true
      }
    }
  }